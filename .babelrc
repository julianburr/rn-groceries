{
  "presets": ["react-native-stage-0/decorator-support"],
  "plugins" : [
    [
      "module-resolver",
      {
        "alias": {
          "app": "./app",
          "screens": "./app/screens",
          "components": "./app/components",
          "containers": "./app/containers",
          "utils": "./app/utils",
          "actions": "./app/actions",
          "reducers": "./app/reducers"
        }
      }
    ]
  ]
}